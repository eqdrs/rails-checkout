<% company.errors.full_messages.each do |message| %>
  <%= message %>
<% end %>

<%= form_for company, class: "ls-form row", local: true do |f| %>
 <fieldset>
  <div class="ls-label col-md-5">
  <%= f.label :company_name, class: "ls-label-text"%>
  <%= f.text_field :company_name, class: 'form-control col-md-6' %>
  </div>
  <div class="ls-label col-md-5">
  <%= f.label :email, class: "ls-label-text" %>
  <%= f.text_area :email, class: 'form-control col-md-6'  %>
  </div>
  <div class="ls-label col-md-5">
  <%= f.label :address, class: "ls-label-text" %>
  <%= f.text_field :address, class: 'form-control col-md-6' %>
  </div>
  <div class="ls-label col-md-5">
  <%= f.label :cnpj, class: "ls-label-text" %>
  <%= f.number_field :cnpj, class: 'form-control col-md-6' %>
  </div>
  <div class="ls-label col-md-5">
  <%= f.label :phone, 'Telefone', class: "ls-label-text" %>
  <%= f.text_field :phone, class: 'form-control col-md-6' %>
  </div>
  <div class="ls-label col-md-5">
  <%= f.label :contact, class: "ls-label-text"%>
  <%= f.text_field :contact, class: 'form-control col-md-6' %>
  </div>
  </fieldset>
    <div class="ls-actions-btn">
  <%= f.submit 'Cadastrar', class: 'ls-btn-primary' %>
  </div>
   
<% end %>